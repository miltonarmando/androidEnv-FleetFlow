<RelatorioScreen>:
    name: 'tabela'
    MDFloatLayout:
        md_bg_color: 188/255, 227/255, 217/255, 1
        MDIconButton:
            icon: "chevron-left"
            user_font_size: "35sp"
            pos_hint: {"center_y": .95}
            on_press: app.root.current = 'dashboard'     

        MDDropDownItem:
            id: dropdown_item
            pos_hint: {'center_x': 0.33, 'center_y': 0.95}
            size_hint_x: None
            width: "200dp"
            text: " Global"
            on_release: root.show_dropdown_options()

        MDIconButton:
            icon: "beaker-plus"
            theme_text_color: 'Custom'
            text_color: (0, 0.5, 0, 0.65)
            user_font_size: "35sp"
            pos_hint: {"center_y": .95, "center_x": 0.75}
            on_press: root.filtrar_receitas()

        MDIconButton:
            icon: "beaker-minus"
            theme_text_color: 'Custom'
            text_color: (1, 0, 0, 0.7)
            user_font_size: "35sp"
            pos_hint: {"center_y": .95, "center_x": 0.9}
            on_press: root.filtrar_despesas()
        
        MDIconButton:
            icon: "filter-remove"
            user_font_size: "35sp"
            pos_hint: {"center_y": .95, "center_x": 0.60}
            on_press: root.filtrar_tudo()

        BoxLayout:
            size_hint_y: 0.55
            pos_hint: {"center_x": 0.5, "center_y": 0.665}
            md_bg_color: 0, 0, 0, 0.0
            MDCard:
                size_hint: 1, 0.07
                pos_hint: {"center_x": 0.5, "center_y": 0.865}
                md_bg_color: 0, 0, 0, 0
                orientation: 'vertical'
                MDLabel:
                    text: root.matriculax
                    theme_text_color: 'Custom'
                    text_color: 0.3, 0.3, 0.3, 0.65
                    font_size: '14sp'
                    halign: 'center'
                    bold: False

        BoxLayout:
            orientation: 'horizontal'
            pos_hint: {"center_x": 0.5, "center_y": 0.85}
            size_hint_x: 0.95

            MDCard:
                size_hint: .9, .07
                pos_hint: {"center_x": 0.9, "center_y": 0.5}
                md_bg_color: 0, 1, 0, 0.0
                orientation: 'vertical'
                MDLabel:
                    text: root.somaReceitas
                    theme_text_color: 'Custom'
                    text_color: 0.3, 0.3, 0.3, 1
                    font_size: '13.5sp'
                    halign: 'center'
                MDLabel:
                    text: 'Total de Receitas'
                    theme_text_color: 'Custom'
                    text_color: 0.3, 0.3, 0.3, 0.65
                    font_size: '12.5sp'
                    halign: 'center'
            MDCard:
                size_hint: .9, .07
                pos_hint: {"center_x": 0.9, "center_y": 0.44}
                md_bg_color: 0, 1, 0, 0.0 
                orientation: 'vertical'
                MDLabel:
                    text: root.lucro
                    theme_text_color: 'Custom'
                    text_color: (0.3, 0.3, 0.3, 1) if root.statusA=='aderente' else (0.8, 0.4, 0, 0.75)
                    font_size: '15sp'
                    halign: 'center'
                    bold: True
                MDLabel:
                    text: 'Total de Lucro'
                    theme_text_color: 'Custom'
                    text_color: 0.3, 0.3, 0.3, 0.65
                    font_size: '12.5sp'
                    halign: 'center'

            MDCard:
                size_hint: .9, .07
                pos_hint: {"center_x": 0.1, "center_y": 0.5}
                md_bg_color: 0, 1, 0, 0.0
                orientation: 'vertical'
                MDLabel:
                    text: root.somaDespesas
                    theme_text_color: 'Custom'
                    text_color: 0.3, 0.3, 0.3, 1
                    font_size: '13.5sp'
                    halign: 'center'
                MDLabel:
                    text: 'Total de Despesas'
                    theme_text_color: 'Custom'
                    text_color: 0.3, 0.3, 0.3, 0.65
                    font_size: '12.5sp'
                    halign: 'center'
            
    MDBoxLayout:
        size_hint_y: 0.75
        orientation: 'vertical'
        ScrollView:
            MDList:
                id: layout

